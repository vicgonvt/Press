{
    "name": "vicgonvt/press",
    "description": "An elegant markdown-powered blog for the Laravel framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Gonzalez",
            "email": "vicgonvt@gmail.com"
        }
    ],
    "require": {
        "erusev/parsedown": "^1.7",
        "php": "^7.2",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "vicgonvt\\Press\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "vicgonvt\\Press\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "vicgonvt\\Press\\PressBaseServiceProvider"
            ]
        }
    }
}
